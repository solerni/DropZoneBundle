{% extends 'ClarolineCoreBundle:Workspace:layout.html.twig' %}

{% block stylesheets %}
    {{ parent() }}
    {% stylesheets debug=false filter='lessphp'  output='bundles/icapdropzone/css/style.css'
      "@IcapDropzoneBundle/Resources/views/less/style.less"
    %}
        <link rel="stylesheet" href="{{ asset_url }}" screen="media" />
        <link href="{{ asset('bundles/icapdropzone/css/solerni.css') }}" type="text/css" rel="stylesheet"/>
        
    {% endstylesheets %}
{% endblock %}

{% block section_content %}
    {% block dropzone_content %}
    {% endblock %}
{% endblock %}

{% block javascripts %}
    {{ parent() }}
        <script src="{{ asset('bundles/icapdropzone/js/jquery.collapse.js') }}"></script>
{% endblock %}